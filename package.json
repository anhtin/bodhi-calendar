{
  "name": "bodhi-calendar-v2",
  "version": "2022-09-22.0",
  "private": true,
  "homepage": "https://anhtin.github.io/bodhi-calendar",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --fix src/**/*.{tsx,ts}",
    "format": "prettier --write src/**/*.{tsx,ts}",
    "predeploy": "npm run lint && CI=true npm run test --passWithNoTests && node ./scripts/bump_version.js && npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@sentry/browser": "5.6.3",
    "amlich.js": "1.0.6",
    "date-fns": "^2.29.3",
    "get-user-locale": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.15.1",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-modal": "^3.10.6",
    "@types/styled-components": "^5.1.26",
    "gh-pages": "^3.1.0"
  }
}
